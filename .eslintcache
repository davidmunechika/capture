vidmunechika/Documents/VS Code/Courses/DevEd/Capture/capture/src/pages/MovieDetail.js",["70"],"import React, { useState, useEffect } from 'react';\nimport styled from 'styled-components';\nimport { useHistory } from 'react-router-dom';\nimport { MovieState } from '../movieState';\n//Animations\nimport { motion } from 'framer-motion';\nimport { pageAnimation } from '../animation';\n\nconst MovieDetail = () => {\n  const history = useHistory();\n  const url = history.location.pathname;\n  const [movies, setMovies] = useState(MovieState);\n  const [movie, setMovie] = useState(null);\n\n  //UseEffect\n  useEffect(() => {\n    const currentMovie = movies.filter((stateMovie) => stateMovie.url === url);\n    setMovie(currentMovie[0]);\n  }, [movies, url]);\n\n  return (\n    <>\n      {movie && (\n        <Details\n          exit=\"exit\"\n          variants={pageAnimation}\n          initial=\"hidden\"\n          animate=\"show\"\n        >\n          <HeadLine>\n            <h2>{movie.title}</h2>\n            <img src={movie.mainImg} alt=\"movie\" />\n          </HeadLine>\n          <Awards>\n            {movie.awards.map((award) => (\n              <Award\n                title={award.title}\n                description={award.description}\n                key={award.title}\n              />\n            ))}\n          </Awards>\n          <ImageDisplay>\n            <img src={movie.secondaryImg} alt=\"movie\" />\n          </ImageDisplay>\n        </Details>\n      )}\n    </>\n  );\n};\n\nconst Details = styled(motion.div)`\n  color: white;\n`;\nconst HeadLine = styled.div`\n  min-height: 90vh;\n  padding-top: 20vh;\n  position: relative;\n  h2 {\n    position: absolute;\n    top: 10%;\n    left: 50%;\n    transform: translate(-50%, -10%);\n  }\n  img {\n    width: 100%;\n    height: 70vh;\n    object-fit: cover;\n  }\n`;\nconst Awards = styled.div`\n  min-height: 80vh;\n  display: flex;\n  margin: 5rem 10rem;\n  align-items: center;\n  justify-content: space-around;\n`;\nconst AwardStyle = styled.div`\n  padding: 5rem;\n  h3 {\n    font-size: 2rem;\n  }\n  .line {\n    width: 100%;\n    background: #23d997;\n    height: 0.5rem;\n    margin: 1rem 0rem;\n  }\n  p {\n    padding: 2rem 0rem;\n  }\n`;\nconst ImageDisplay = styled.div`\n  min-height: 50vh;\n  img {\n    width: 100%;\n    height: 100vh;\n    object-fit: cover;\n  }\n`;\n\n//Award Component\nconst Award = ({ title, description }) => {\n  return (\n    <AwardStyle>\n      <h3>{title}</h3>\n      <div className=\"line\"></div>\n      <p>{description}</p>\n    </AwardStyle>\n  );\n};\n\nexport default MovieDetail;\n","/Users/davidmunechika/Documents/VS Code/Courses/DevEd/Capture/capture/src/movieState.js",[],"/Users/davidmunechika/Documents/VS Code/Courses/DevEd/Capture/capture/src/animation.js",[],"/Users/davidmunechika/Documents/VS Code/Courses/DevEd/Capture/capture/src/components/Wave.js",[],"/Users/davidmunechika/Documents/VS Code/Courses/DevEd/Capture/capture/src/components/Toggle.js",[],{"ruleId":"71","replacedBy":"72"},{"ruleId":"73","replacedBy":"74"},{"ruleId":"75","severity":1,"message":"76","line":12,"column":18,"nodeType":"77","messageId":"78","endLine":12,"endColumn":27},"no-native-reassign",["79"],"no-negated-in-lhs",["80"],"no-unused-vars","'setMovies' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]