Detail;\n","/Users/davidmunechika/Documents/VS Code/Courses/DevEd/Capture/capture/src/movieState.js",[],"/Users/davidmunechika/Documents/VS Code/Courses/DevEd/Capture/capture/src/animation.js",[],{"ruleId":"66","replacedBy":"67"},{"ruleId":"68","replacedBy":"69"},{"ruleId":"66","replacedBy":"70"},{"ruleId":"68","replacedBy":"71"},{"ruleId":"72","severity":1,"message":"73","line":12,"column":18,"nodeType":"74","messageId":"75","endLine":12,"endColumn":27},"no-native-reassign",["76"],"no-negated-in-lhs",["77"],["76"],["77"],"no-unused-vars","'setMovies' is assigned a value but never used.","Identifier","unusedVar","no-global-assign","no-unsafe-negation"]